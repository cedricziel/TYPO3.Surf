{
  "alias": "surf.phar",
  "directories": [
    "src/"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        ".gitignore",
        ".md",
        "phpunit",
        "Tester",
        "Tests",
        "tests",
        "yaml"
      ],
      "in": "vendor"
    }
  ],
  "compactors": [
    "Herrera\\Box\\Compactor\\Json",
    "Herrera\\Box\\Compactor\\Php"
  ],
  "compression": "GZ",
  "git-version": "package_version",
  "main": "surf.php",
  "output": "surf.phar",
  "stub": true,
  "chmod": "0755"
}